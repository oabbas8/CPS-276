<?php


    $i = 0;
    $j = 0;

    $table = "<table border='1'>";

    while($i <= 15) { //row count
        $table .= "<tr>";
        $j = 0;
    while($j < 5) { //column count
        $table .= "<td>Cell</td>";
        $j++;
            
    }
   
    $i++;
    $table.= "</tr>";
}
    $table .= "</table>";

?>


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Exercise 3</title>
</head>
<body>
        <p><?php echo $table; ?></p>
</body>
</html>
